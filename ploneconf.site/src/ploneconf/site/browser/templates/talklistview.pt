<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="ploneconf.site">
<body>

<metal:content-core fill-slot="content-core">


<table class="listing">
    <tr>
        <th>Title</th>
        <th>Descriptions</th>
    </tr>

    <tr tal:repeat="talk python:view.talks()">
        <td>
            <a href="${python:talk['url']}">
                ${python:talk['title']}
            </a>
       </td>
       <td>
            <p>
                ${python:talk['description']}
            </p>
       </td>
    </tr>
</table>

<table class="listing">
    <tr>
        <th>Title</th>
        <th>Descriptions</th>
    </tr>

    <tr tal:repeat="talk python:view.get_news()">
        <td>
            <a href="${python:talk['url']}">
                ${python:talk['title']}
            </a>
       </td>
       <td>
            <p>
                ${python:talk['description']}
            </p>
       </td>
    </tr>
</table>



</metal:content-core>

</body>
</html>
